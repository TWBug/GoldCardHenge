{{ $icon144 := resources.Get "img/android-icon-144x144.png" | fingerprint }}
{{ $icon192 := resources.Get "img/android-icon-192x192.png" | fingerprint }}
{
    "name": "{{ .Site.Title }}",
    "short_name": "{{ .Site.Data.variables.Plugin.Description }}",
    "start_url": "{{ .Site.BaseURL }}",
    "icons": [
        {
            "src": "{{ $icon144.RelPermalink }}",
            "sizes": "144x144",
            "type": "image/png"
        },
        {
            "src": "{{ $icon192.RelPermalink }}",
            "sizes": "192x192",
            "type": "image/png"
        }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "display": "standalone"
}